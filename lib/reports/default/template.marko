<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8"/>
        <title>${data.title} | http-stats</title>
    </head>
    <body>
        <div id="reqPerSecContainer"
            style="min-width: 310px; height: 300px; margin: 0 auto"/>
        <div id="responseTimeContainer"
            style="min-width: 310px; height: 300px; margin: 0 auto"/>
        <div id="cpuContainer" style="min-width: 310px; height: 300px; margin: 0 auto"
            if(data.showCPU)/>
        <div id="memoryContainer"
            style="min-width: 310px; height: 300px; margin: 0 auto"
            if(data.showMemory)/>
        <embed-file path=require.resolve("jquery/dist/jquery.min.js")/>
        <embed-file path="../../../third-party/highcharts/js/highcharts.js"/>
        <embed-file path="../../../third-party/highcharts/js/modules/exporting.js"/>
        <embed-file path="./http-stats-report-client.js"/>
        <script type="text/javascript" space="preserve">
            window.showHttpStatsReport($!{data.results});
        </script>
    </body>
</html>
